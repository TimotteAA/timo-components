    "rollup-plugin-sass": "^1.2.13",
    "rollup-plugin-typescript2": "^0.32.1",
    "sass": "^1.53.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "main": "dist/bundle.es.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "Test",
    "React"
